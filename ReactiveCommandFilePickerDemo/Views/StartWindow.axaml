<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="ReactiveCommandFilePickerDemo.Views.StartWindow"
        Title="StartWindow">
  <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Vertical">
    <Button  Content="Select File using ReactiveCommand"
                Margin="20"
                Command="{Binding ReactiveOpenFileCommand}"/>
                
    <Button  Content="Select File using ICommand"
                Margin="20"
                Command="{Binding ICommandOpenFileCommand}"/>
  </StackPanel>
</Window>
